   "metadata": {},
   "source": [
    "### Code:"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 5,
   "id": "b8ead919",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Enter your string: MadrID\n",
      "adrMID\n"
     ]
    }
   ],
   "source": [
    "x = str(input(\"Enter your string: \"))\n",
    "y = \"\"\n",
    "z = \"\"\n",
    "\n",
    "\n",
    "for i in x:\n",
    "    if i.islower():\n",
    "        y+=i\n",
    "    else:\n",
    "        z+=i\n",
    "z = (y+z)\n",
    "print(z)"
   ]
